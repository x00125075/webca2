GET     /dogsbay                    controllers.HomeController.dogsbay
GET     /keembay                    controllers.HomeController.keembay
GET     /coumeenole                 controllers.HomeController.coumeenole
GET     /skerries                   controllers.HomeController.skerries
GET     /sharmelsheikh              controllers.HomeController.sharmelsheikh
GET     /cancun                     controllers.HomeController.cancun
GET     /barbados                   controllers.HomeController.barbados
GET     /maldives                   controllers.HomeController.maldives
GET     /dominican                  controllers.HomeController.dominican
GET     /bali                       controllers.HomeController.bali
GET     /polynesia                  controllers.HomeController.polynesia
